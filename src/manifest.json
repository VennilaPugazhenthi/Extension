{
  "name": "SearchThroughHyperlinks",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Search a Word in all the Hyperlinks in a particular URL!",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "SearchThroughHyperlinks"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs"]
}
